            xmlns:prism="clr-namespace:Prism.Mvvm;assembly=Prism.Forms"
            prism:ViewModelLocator.AutowireViewModel="True"
            xmlns:local="clr-namespace:PrismXamarinForms.Views;assembly=PrismXamarinForms"
            x:Class="PrismXamarinForms.Views.PrismTabbedPage"
            Title="{Binding Titulo}">

    <TabbedPage.Children>
        <local:PrismPageA />
        <local:PrismPageB />
        <local:PrismPageC />
    </TabbedPage.Children>
    
</TabbedPage>